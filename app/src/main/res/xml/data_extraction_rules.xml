<?xml version="1.0" encoding="utf-8"?>
<data-extraction-rules>
    <cloud-backup>
        <!-- Allow backup of app preferences -->
        <include domain="sharedpref" path="."/>
    </cloud-backup>
    
    <device-transfer>
        <!-- Allow transfer of app data between devices -->
        <include domain="sharedpref" path="."/>
    </device-transfer>
</data-extraction-rules>